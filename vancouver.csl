<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0">
  <info>
    <title>Custom Numbered – Italic Journal</title>
    <id>http://www.zotero.org/styles/custom-numbered-italic-journal</id>
    <link href="http://www.zotero.org/styles/custom-numbered-italic-journal" rel="self"/>
    <author><name>Generated by ChatGPT</name></author>
    <category citation-format="numeric"/>
    <category field="generic-base"/>
  </info>

  <macro name="author">
    <names variable="author">
      <name form="short" delimiter=", "/>
      <label form="short" prefix=", " suffix="."/>
    </names>
  </macro>

  <macro name="title">
    <text variable="title" suffix=","/>
  </macro>

  <macro name="journal">
    <text variable="container-title" font-style="italic" suffix="., "/>
  </macro>

  <macro name="volume">
    <text variable="volume" suffix=" "/>
  </macro>

  <macro name="issue-year">
    <group delimiter=" ">
      <text variable="issue" prefix="(" suffix=")"/>
      <text variable="issued" form="year" prefix="(" suffix=")"/>
    </group>
  </macro>

  <macro name="pages">
    <text variable="page" suffix="."/>
  </macro>

  <citation>
    <layout prefix="[" suffix="]" delimiter="; ">
      <text variable="citation-number"/>
    </layout>
  </citation>

  <bibliography>
    <layout hanging-indent="true" delimiter=" ">
      <text macro="author"/>
      <text macro="title"/>
      <text macro="journal"/>
      <text macro="volume"/>
      <text macro="issue-year"/>
      <text macro="pages"/>
    </layout>
  </bibliography>
</style>
